            url: '/AssetCategory/SupplierList',
            type: 'POST',
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (response) {
                if (response.length > 0) {
                    console.log(response);
                    $('select#SupplierList_mod').html('');
                    var result = '';
                    result += "<option value='0'>Supplier Name</option>";
                    for (var i = 0; response.length > i; i++) {

                        result += "<option id='" + response[i].SUPPLIER_ID + "'  data-STORAGE_ID='" + response[i].EXTERNAL_ID + "' title='" + response[i].NAME + "' value='" + response[i].SUPPLIER_ID + "' data-shortname='" + response[i].STATUS + "'>" + response[i].NAME + "</option>";

                    }
                }
                $('select#SupplierList_mod').append(result);


            }
        });
    }


    function AssetList(segmentId, name) {
        $.ajax({
            url: '/AssetCategory/AssetList',
            type: 'POST',
            contentType: "application/json;charset=utf-8",
            data: JSON.stringify({ _segmentId: segmentId, _name: name }),
            dataType: "json",
            success: function (response) {

                if (response.length > 0) {
                    $('select#asset_listB').html('');


                    var result = '';

                    for (var i = response.length - 1; response.length > i; i++) {

                        result += "<option id='" + response[i].ASSET_CODE + "' data-shortname='" + response[i].MST_TAG + "' value='" + response[i].ASSET_CODE + "'>" + response[i].NAME + "</option>";

                    }

                    $('select#asset_listB').append(result);

                }
            }
        });
    }
    function getPeripheralData() {


        $.ajax({

            type: "GET",
            url: '/AssetCategory/PeripheralGridList',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {

                if (response != null) {

                    $('#peripheralList').empty();

                    var result = '';
                    if (response.data.length > 0) {

                        result += '<div class="col-md-12">';
                        result += '<div class="box-body table-responsive no-padding" id="peripheraldatatableload">';
                        result += '<table id="peripheraldatatable" class="table table-bordered table-striped compact hover" cellspacing="0" width="100%">';
                        result += '<thead>';
                        result += '<tr>';
                        result += '<th>Class</th>';
                        result += '<th>Category</th>';
                        result += '<th>Sub Category</th>';
                        result += '<th>Peripheral</th>';
                        result += '<th>Quantity</th>';
                        result += '<th>Actions</th>';
                        result += '</tr>';
                        result += '</thead>';
                        result += '<tbody>';

                        for (var i = 0; i < response.data.length; i++) {


                            result += '<tr id="' + response.data[i].ASSET_CODE + '">';
                            result += '<td> ' + response.data[i].ASSET_CLASS_NAME + '</td>';
                            result += '<td> ' + response.data[i].CATEGORY_NAME + '</td>';
                            result += '<td > ' + response.data[i].SEGMENT_NAME + '</td>';
                            result += '<td > ' + response.data[i].DESCRIPTION + '</td>';
                            result += '<td> ' + response.data[i].QUANTITY + '</td>';
                            var status = response.data[i].STATUS;
                            if (status != 2) {
                                result += '<td> <button type="button" class="btn btn-primary" onclick="deleteAssetPeripheral(' + response.data[i].ASSET_CODE + ')">Delete</button></td>';
                            } else {
                                result += '<td></td>';
                            }

                            result += '</tr>';

                        }
                        result += '</tbody>';
                        result += '</table>';
                        result += '</div>';
                        result += '</div>';
                    }
                    $('#peripheralList').append(result);
                }

            },
            error: function () {
                alert("error");
            }
        });
    }
