    function deleteAssetPeripheral(PeripheralCode) {
        Swal.fire({
            title: 'DO YOU WANT TO DELETE THIS?',
            showDenyButton: true,
            confirmButtonText: 'D E L E T E',
            denyButtonText: 'N O',
        }).then((result) => {
            if (result.isConfirmed) {
                $.ajax({
                    type: "POST",
                    url: "/Department/Deleteperipheral",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    data: JSON.stringify({

                        PeripheralCode: PeripheralCode

                    }),
                    success: function (result) {
                        getPeripheralData();
                        getcomponentdatatable();

                        $('#collapseSix').addClass('in');
                        $('#collapseSix').attr('aria-expanded', 'true');

                        $("h2").css({ color: result[1] });
                    },
                    error: function () {

                        Toast.fire({
                            icon: 'error',
                            title: 'error'
                        });

                    }
                })
            } else if (result.isDenied) {
                Swal.fire('NO ITEM HAS BEEN DELETED', '', 'info')
            }
        });
    }

